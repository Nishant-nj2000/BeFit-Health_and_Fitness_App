<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.befit_healthandfitnessapplication.Home_fragment"
        android:label="@string/home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_plans"
        android:name="com.example.befit_healthandfitnessapplication.Plan_Fragment"
        android:label="@string/Training_Plans"
        tools:layout="@layout/plans_fragment" />

    <fragment
        android:id="@+id/nav_reports"
        android:name="com.example.befit_healthandfitnessapplication.Report_fragment"
        android:label="@string/reports"
        tools:layout="@layout/reports_fragment" />

    <fragment
        android:id="@+id/nav_goals"
        android:name="com.example.befit_healthandfitnessapplication.Goal_Fragment"
        android:label="@string/goals"
        tools:layout="@layout/goals_fragment" />

    <fragment
        android:id="@+id/nav_settings"
        android:name="com.example.befit_healthandfitnessapplication.Settings_fragment"
        android:label="@string/settings"
        tools:layout="@layout/settings_fragment" />

    <fragment
        android:id="@+id/nav_logout"
        android:name="com.example.befit_healthandfitnessapplication.logout"
        android:label="Logout"
        tools:layout="@layout/activity_logout" />

</navigation>